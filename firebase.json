{
  "firebase": "beckme",
  "public": "public",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "headers":[ {
    "source" : "**/*.@(jpg|jpeg|gif|png)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
  } ]
  },{
    "source" : "**/*.@(css)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
    } ]
  },
  {
	"source" : "**/*.@(mp4)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
    } ]
  },
  {
  "source" : "**/*.@(js)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
    } ]
  },
  {
  	"source" : "**/*.@(ttf)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
    } ]

  }]
}